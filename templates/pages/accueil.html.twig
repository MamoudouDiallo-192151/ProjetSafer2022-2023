							<div class="property-controls">
								<ul>
									<li data-filter="all">Categories</li>
									<li data-filter=".apart"></li>

								</ul>
							</div>
						</div>
					</div>
					<div class="row property-filter">

						{% for bien in biens %}
							<div class="col-lg-4 col-md-6 mix all house">
								<div class="property-item">
									<div class="pi-pic set-bg" {% if bien.image %} data-setbg="{{ vich_uploader_asset(bien,'imageFile') | imagine_filter('my_thumb') }}" alt="imgcache" style=" width: 90%; height: 200px;">
									{% endif %}
									{% if bien.statusBien==1 %}
										<div class="label c-red">En vente</div>
									{% else %}
										<div class="label">
											En Location
										</div>

									{% endif %}
								</div>
								<div class="pi-text">
									<a href="#" class="heart-icon">
										<span class="icon_heart_alt"></span>
									</a>
									<div class="pt-price">{{bien.prix}}Â£<span>
											{% if bien.statusBien==0 %}/mois
											{% endif %}
										</span>
									</div>
									<h5>
										<a href="#">{{bien.titre}}</a>
									</h5>
									<p>
										<span class="icon_pin_alt"></span>
										{{bien.localisation}}</p>
									<ul>
										<li>
											<i class="fa fa-object-group"></i>
											{{bien.surface}}
